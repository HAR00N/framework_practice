<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="testMapper">  
    <select id="getAll">
        SELECT *
        FROM framework_resource
    </select>
    <select id="getId">
        SELECT *
        FROM framework_resource
        WHERE user_id = '${id}'
    </select>
</mapper>